    "eslint-plugin-json-files": "0.8.0",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "fixturify-project": "^1.9.0",
    "git-fixtures": "^2.3.0",
    "mocha": "^7.0.0",
