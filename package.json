    "fixturify": "^1.0.0",
    "fs-extra": "^8.0.0",
    "tmp": "0.1.0",
    "uuid": "^3.1.0",
    "yargs": "^15.0.0"
  },
  "devDependencies": {
    "@kellyselden/node-template": "0.8.4",
    "chai": "^4.2.0",
    "cpr": "^3.0.1",
